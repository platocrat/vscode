{
    "terminal.integrated.rendererType": "dom",
    "files.autoSave": "afterDelay",
    "editor.tabSize": 2,
    "editor.accessibilityPageSize": 14,
    "editor.formatOnSave": true,
    "editor.lineHeight": 23,
    "editor.tokenColorCustomizations": null,
    "editor.fontLigatures": null,
    "editor.rulers": [
        80,
        120
    ],
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
    "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "window.autoDetectColorScheme": true, 
    "workbench.sideBar.location": "right",
    "zenMode.restore": true,
    "window.doubleClickIconToClose": true,
    "window.closeWhenEmpty": true,
    "window.restoreFullscreen": true,
    "problems.showCurrentInStatus": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.zoomLevel": 1,
    "diffEditor.codeLens": true,
    "workbench.sash.size": 8,
    "workbench.colorCustomizations": {
        "statusBar.background": "#ffc7c7",
        "statusBar.noFolderBackground": "#ffc7c7",
        "statusBar.debuggingBackground": "#ffc7c7",
        "titleBar.activeBackground": "#191919cc",
        "titleBar.activeForeground": "#b4647c21",
        "editor.foreground": "#EDE1E8",
        "activityBar.background": "#aa4e6970",
        "activityBar.foreground": "#FEE5F3",
        "sideBarSectionHeader.background": "#ffc7c71c",
        "button.background": "#b45b71d0",
        "list.activeSelectionBackground": "#70512d",
        "list.hoverBackground": "#ffb9b910",
        "input.border": "#b48c1f36",
        "inputOption.activeBorder": "#cf7685",
        "focusBorder": "#ffc7c78f",
        "editor.selectionBackground": "#d874b1",
        "editor.selectionHighlightBackground": "#ffffff11",
        "scrollbarSlider.activeBackground": "#4fa8a1",
        "scrollbarSlider.hoverBackground": "#6bcfd899",
        "editorCursor.background": "#ffffff6e",
        "editorCursor.foreground": "#ffffff",
        "settings.checkboxBorder": "#d18b8bad",
        "selection.background": "#b99a9a",
        "editor.lineHighlightBackground": "#ffffff11",
        "editor.selectionHighlightBorder": "#ffffff2c",
    },
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "Material Theme Ocean",
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "files.exclude": {
        "**/.node_modules": true
    },
    "editor.fontSize": 12.2,
    "python.languageServer": "Microsoft",
    "rust.all_features": true,
    "editor.showUnused": false,
    "editor.quickSuggestions": true,
    "go.formatTool": "goimports"
}
